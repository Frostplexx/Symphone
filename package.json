{
  "name": "Symphone",
  "productName": "Symphone",
  "version": "0.3.0",
  "description": "A graphical user interface for the python spotify_dl and youtube-dl command line tool",
  "main": "src/index.js",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Frostplexx",
    "email": "daniel.inama02@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "SpotifyDownloader"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "ansi-regex": ">=5.0.1",
    "bulma-extensions": "^6.2.7",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "ffmpeg-static-electron": "^2.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "glasstron": "^0.1.0",
    "http-proxy": ">=1.18.1",
    "node-fetch": "^3.2.10",
    "remote": "^0.2.6",
    "request": "^2.88.2",
    "scrape-youtube": "^2.1.9",
    "spotify-web-api-node": "^5.0.2",
    "trim-newlines": ">=3.0.1",
    "underscore": ">=1.12.1",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "babel-preset-stage-0": "^6.24.1",
    "bulma": "^0.9.2",
    "electron": "15.5.5",
    "electron-packager": "^15.3.0",
    "node-sass": "^7.0.0"
  }
}
